   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "number\tsquare\tcube\n",
      "0 \t 0 \t 0\n",
      "1 \t 1 \t 1\n",
      "2 \t 4 \t 8\n",
      "3 \t 9 \t 27\n",
      "4 \t 16 \t 64\n",
      "5 \t 25 \t 125\n"
     ]
    }
   ],
   "source": [
    "print('number\\tsquare\\tcube')\n",
    "print(0, '\\t', 0 * 0, '\\t', 0 * 0 * 0)\n",
    "print(1, '\\t', 1 * 1, '\\t', 1 * 1 * 1)\n",
    "print(2, '\\t', 2 * 2, '\\t', 2 * 2 * 2)\n",
    "print(3, '\\t', 3 * 3, '\\t', 3 * 3 * 3)\n",
    "print(4, '\\t', 4 * 4, '\\t', 4 * 4 * 4)\n",
    "print(5, '\\t', 5 * 5, '\\t', 5 * 5 * 5)"
   ]
  }
